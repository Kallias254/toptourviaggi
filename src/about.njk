    <!--====== Start Team Section ======-->
<section class="team-section pt-100 pb-70 gray-bg">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-xl-7">
                <!--====== Section Title ======-->
                <div class="section-title text-center mb-50 wow fadeInDown">
                    <span class="sub-title">Meet Our Team</span>
                    <h2>Discover the People Behind Our Success</h2>
                    <p>Our team brings together passion, expertise, and creativity to deliver exceptional experiences.</p>
                </div>
            </div>
        </div>
        <div class="row">
            {% for member in team.team %}
            <div class="col-lg-4 col-md-6 col-sm-12">
                <!--====== Single Team Item ======-->
                <div class="single-team-item mb-30 wow fadeInUp">
                    <div class="member-img">
                        <img src="{{ member.image }}" alt="{{ member.name }}">
                    </div>
                    <div class="member-info">
                        <h4 class="title">{{ member.name }}</h4>
                        <p class="position">{{ member.position }}</p>
                        <ul class="social-link">
                            <li><a href="{{ member.social.facebook }}"><i class="fab fa-facebook-f"></i></a></li>
                            <li><a href="{{ member.social.twitter }}"><i class="fab fa-twitter"></i></a></li>
                            <li><a href="{{ member.social.instagram }}"><i class="fab fa-instagram"></i></a></li>
                            <li><a href="{{ member.social.linkedin }}"><i class="fab fa-linkedin-in"></i></a></li>
                        </ul>
                    </div>
                </div>
            </div>
            {% endfor %}
        </div>
    </div>
</section><!--====== End Team Section ======-->

<!--====== Start CTA Section ======-->
{# <section class="cta-bg overlay bg_cover pt-150 pb-150" style="background-image: url(/assets/images/bg/cta-bg.jpg);"> #}
<section class="cta-bg overlay bg_cover pt-150 pb-150" style="background-image: url('{{ aboutUsPage.ctaBackground[0].src }}');">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-xl-8">
                <!--=== CTA Content Box ===-->
                <div class="cta-content-box text-center text-white wow fadeInDown">
                    <h2 class="mb-35">Embark on a Journey of Discovery</h2>
                    <p class="mb-35 fs-4">Experience the thrill of adventure in breathtaking destinations. Are you ready to explore the wonders of the world with us?</p>
                    <a href="/destinations" class="main-btn primary-btn">Check Availability<i class="far fa-paper-plane"></i></a>
                </div>
            </div>
        </div>
    </div>
</section><!--====== End CTA Section ======-->


    <!--====== Start Testimonial Section ======-->

        <!--====== Start Testimonial Section ======-->
        <section class="testimonial-section pt-60 pb-60">
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-xl-8">
                        <!--=== Section Title ===-->
                        <div class="section-title text-center mb-60 wow fadeInDown">
                            <span class="sub-title">Testimonials</span>
                            <h2>Hear from Our Happy Adventurers</h2>
                        </div>
                    </div>
                </div>
                <div class="row align-items-xl-center">
                    <div class="col-xl-7 col-lg-12 order-1 order-xl-2">
                        <!--=== Testimonial Slider ===-->
                        <div class="testimonial-slider-one pl-lg-55 mb-40 wow fadeInRight">
                            {% for testimonial in testimonials.testimonials %}
    
                            <!--=== Testimonial Item ===-->
                            <div class="gw-testimonial-item">
                                <div class="testimonial-inner-content">
                                    <div class="quote-rating-box">
                                        <div class="icon">
                                            <img src="/assets/images/testimonial/quote.png" alt="quote icon">
                                        </div>
                                        <div class="ratings-box">
                                            <h4>{{ testimonial.service }}</h4>
                                            <ul class="ratings">
                                                {% for i in range(0, testimonial.rating) %}
                                                <li><i class="fas fa-star"></i></li>
                                                {% endfor %}
                                            </ul>
                                        </div>
                                    </div>
                                    <p>{{ testimonial.text }}</p>
                                    <div class="author-thumb-title">
                                        <div class="author-thumb">
                                            <img src="{{ testimonial.author.image }}" alt="Image of {{ testimonial.author.name }}">
                                        </div>
                                        <div class="author-title">
                                            <h3 class="title">{{ testimonial.author.name }}</h3>
                                            <p class="position">{{ testimonial.author.position }}</p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            {% endfor %}
                        </div>
                    </div>
                    <div class="col-xl-5 col-lg-12 order-2 order-xl-1">
                        <!--=== Testimonial Image ===-->
                        <div class="testimonial-one_image-box mb-40 wow fadeInLeft">
                            <img src="{{ aboutUsPage.testimonialImage[0].src }}" alt="{{ aboutUsPage.testimonialImage[0].alt }}">
                        </div>
                    </div>
                </div>
            </div>
        </section><!--====== End Testimonial Section ======-->

